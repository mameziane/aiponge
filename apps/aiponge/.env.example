# Aiponge Mobile App (Expo) - Environment Variables
# Source of truth: packages/platform-core/src/config/environment-config.ts

# =============================================================================
# API Configuration
# =============================================================================

# API URL for the backend gateway
EXPO_PUBLIC_API_URL=http://localhost:8080

# =============================================================================
# RevenueCat (In-App Purchases)
# =============================================================================

# EXPO_PUBLIC_REVENUECAT_IOS_KEY=your-ios-key
# EXPO_PUBLIC_REVENUECAT_ANDROID_KEY=your-android-key

# =============================================================================
# Monitoring & Error Tracking
# =============================================================================

# Sentry DSN for mobile error tracking
# EXPO_PUBLIC_SENTRY_DSN=

# =============================================================================
# CI/CD
# =============================================================================

# Expo access token for CI/CD builds
# EXPO_TOKEN=

# =============================================================================
# Development
# =============================================================================

# Development bypass (set to false for real testing)
EXPO_PUBLIC_AUTH_BYPASS=false
EXPO_PUBLIC_USE_FAST_RESOLVER=1
