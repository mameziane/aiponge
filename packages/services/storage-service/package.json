{
  "name": "@aiponge/storage-service",
  "type": "module",
  "version": "1.0.0",
  "description": "Storage service with file management, versioning, access control, and processing capabilities",
  "main": "src/main.ts",
  "types": "src/main.ts",
  "scripts": {
    "build": "node esbuild.config.mjs",
    "dev": "NODE_OPTIONS=\"--conditions=development\" tsx src/main.ts",
    "dev:watch": "NODE_OPTIONS=\"--conditions=development\" tsx watch src/main.ts",
    "start": "node dist/main.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "lint:check": "eslint 'src/**/*.ts' --quiet",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "type-check": "tsc --noEmit --strict",
    "quality-check": "npm run lint:check && npm run format:check && npm run type-check",
    "architectural-lint": "node ../../../scripts/architectural-lint.js",
    "full-quality-check": "npm run quality-check && npm run architectural-lint",
    "clean:all": "rm -rf node_modules apps/*/node_modules packages/*/node_modules .turbo apps/*/.expo && npm cache clean --force",
    "clean:cache": "rm -rf .turbo apps/*/.expo && npm cache clean --force",
    "clean:logs": "find . -name '*.log' -type f -delete"
  },
  "dependencies": {
    "@aiponge/platform-core": "*",
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "multer": "^2.0.2",
    "sharp": "^0.34.5",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
    "zod": "^3.24.1"
  },
  "keywords": [
    "file-management",
    "storage",
    "versioning",
    "access-control",
    "background-processing",
    "resumable-upload",
    "file-search",
    "microservice",
    "s3",
    "cloudinary",
    "gcs",
    "typescript"
  ],
  "author": "aiponge",
  "license": "MIT",
  "devDependencies": {
    "@aiponge/test-utils": "*",
    "@types/multer": "^1.4.7",
    "@types/sharp": "^0.31.1"
  }
}
