{
  "name": "@aiponge/ai-analytics-service",
  "type": "module",
  "version": "1.0.0",
  "description": "Analytics and monitoring microservice for aiponge AI ecosystem - provides comprehensive insights, performance monitoring, and cost tracking",
  "main": "src/main.ts",
  "types": "src/main.ts",
  "scripts": {
    "build": "node esbuild.config.mjs",
    "dev": "NODE_OPTIONS=\"--conditions=development\" tsx src/main.ts",
    "dev:watch": "NODE_OPTIONS=\"--conditions=development\" tsx watch src/main.ts",
    "start": "node dist/main.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "lint:check": "eslint 'src/**/*.ts' --quiet",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "type-check": "tsc --noEmit --strict",
    "quality-check": "npm run lint:check && npm run format:check && npm run type-check",
    "architectural-lint": "node ../../../scripts/architectural-lint.js",
    "full-quality-check": "npm run quality-check && npm run architectural-lint",
    "clean": "rm -rf dist",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@aiponge/platform-core": "*",
    "@aiponge/shared-contracts": "*",
    "axios": "^1.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.5.1",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
    "ws": "^8.14.0",
    "zod": "^3.24.1"
  },
  "keywords": [
    "analytics",
    "monitoring",
    "metrics",
    "timescaledb",
    "real-time-dashboards",
    "cost-optimization",
    "performance-monitoring",
    "workflow-analytics",
    "provider-analytics",
    "health-tracking",
    "microservice",
    "typescript"
  ],
  "author": "aiponge",
  "license": "MIT",
  "devDependencies": {
    "@aiponge/test-utils": "*",
    "@types/pdfkit": "^0.17.4"
  }
}
