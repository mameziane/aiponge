      "capabilities": [
        "image_generation",
        "artwork_creation",
        "album_covers"
      ],
      "requestTemplate": {
        "n": 1,
        "size": "1024x1024",
        "model": "dall-e-3",
        "style": "vivid",
        "prompt": "{{prompt}}",
        "quality": "standard"
      },
      "responseMapping": {
        "imageUrl": "data[0].url",
        "revisedPrompt": "data[0].revised_prompt"
      }
    },
    "is_active": true,
    "is_primary": true,
    "priority": 100,
    "cost_per_unit": 0.000001,
    "health_status": "unknown",
