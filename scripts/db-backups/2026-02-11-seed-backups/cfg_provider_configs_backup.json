[
  {
    "id": 1,
    "provider_id": "musicapi",
    "provider_name": "MusicAPI.ai",
    "provider_type": "music",
    "description": "Advanced music creation with cultural and wellbeing styles",
    "configuration": {
      "cost": 0.000001,
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "timeout": 120000,
      "endpoint": "https://api.musicapi.ai/api/v1/sonic/create",
      "requestTemplate": {
        "mv": "sonic-v5",
        "tags": "{{style}}",
        "title": "{{title}}",
        "prompt": "{{prompt}}",
        "duration": 180,
        "custom_mode": true,
        "make_instrumental": false
      },
      "responseMapping": {
        "message": "message",
        "task_id": "task_id"
      }
    },
