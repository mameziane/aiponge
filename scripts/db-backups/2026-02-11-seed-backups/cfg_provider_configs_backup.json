    "provider_id": "stability-sd",
    "provider_name": "Stability AI Stable Diffusion",
    "provider_type": "image",
    "description": "Stable Diffusion image generation via Stability AI API - fallback for DALL-E",
    "configuration": {
      "auth": {
        "type": "bearer",
        "envVar": "STABILITY_API_KEY"
      },
      "method": "POST",
      "models": [
        "stable-diffusion-xl-1024-v1-0",
        "stable-diffusion-v1-6"
      ],
      "headers": {
        "Accept": "application/json",
        "Content-Type": "application/json"
