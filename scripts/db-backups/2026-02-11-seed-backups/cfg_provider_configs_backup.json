  {
    "id": 2,
    "provider_id": "openai-llm",
    "provider_name": "OpenAI GPT-4o-mini",
    "provider_type": "llm",
    "description": "OpenAI GPT-4o-mini for text generation (lyrics, titles, content)",
    "configuration": {
      "auth": {
        "scheme": "Bearer",
        "envVarName": "OPENAI_API_KEY",
        "headerName": "Authorization"
      },
      "cost": 0.000003,
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "timeout": 180000,
      "endpoint": "https://api.openai.com/v1/chat/completions",
      "requestTemplate": {
        "model": "gpt-4o-mini",
        "messages": [
          {
            "role": "system",
            "content": "${systemPrompt}"
          },
          {
            "role": "user",
            "content": "${userPrompt}"
          }
        ],
        "temperature": 0.7
      },
      "responseMapping": {
        "content": "choices[0].message.content"
      }
    },
    "is_active": true,
    "is_primary": true,
    "priority": 100,
    "cost_per_unit": 0.000003,
    "health_status": "unknown",
    "credit_cost": 1
  },
  {
